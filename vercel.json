{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "headers": [
    {
      "source": "(.*)",
      "headers": [
        {
          "key": "cross-origin-opener-policy",
          "value": "same-origin"
        },
        {
          "key": "cross-origin-embedder-policy",
          "value": "require-corp"
        },
        {
          "key": "cross-origin-resource-policy",
          "value": "cross-origin"
        }
      ]
    }
  ]
}